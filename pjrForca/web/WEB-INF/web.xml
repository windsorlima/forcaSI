<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>CadastroServlet</servlet-name>
        <servlet-class>controller.CadastroServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CadastroServlet</servlet-name>
        <url-pattern>/CadastroServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CadastroPalavraServlet</servlet-name>
        <servlet-class>controller.CadastroPalavraServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CadastroPalavraServlet</servlet-name>
        <url-pattern>/CadastroPalavraServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controller.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ConsultarJogadorServlet</servlet-name>
        <servlet-class>controller.ConsultarJogadorServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConsultarJogadorServlet</servlet-name>
        <url-pattern>/ConsultarJogadorServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ConsultarPalavraServlet</servlet-name>
        <servlet-class>controller.ConsultarPalavraServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConsultarPalavraServlet</servlet-name>
        <url-pattern>/ConsultarPalavraServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeslogarServlet</servlet-name>
        <servlet-class>controller.DeslogarServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeslogarServlet</servlet-name>
        <url-pattern>/DeslogarServlet</url-pattern>
    </servlet-mapping>
</web-app>